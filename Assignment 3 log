108        data PAT_INFO;
109        	merge DEMOG1062new STUDYnew;
110        	by pt site;
111        
112        	pt_id = Site || '-' || Pt;
113        	if missing(site) or missing(pt) then pt_id = ' ';
114        	label pt_id = 'Site-Patient';
